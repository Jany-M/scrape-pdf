{
    "compilerOptions": {
        "target": "ESNext",
        "lib": [
            "ESNext"
        ],
        "allowJs": false,
        "skipLibCheck": false,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "ESNext",
        "moduleResolution": "nodenext",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "sourceMap": true,
        "noEmit": false,
        "outDir": "dist"
    },
    "include": [
        "./scrape_pdf.mts"
    ],
    "exclude": [
        "node_modules"
    ]
}